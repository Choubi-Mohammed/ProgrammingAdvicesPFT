GPP = g++
MAIN = main.cpp
SRC = src/lib.cpp

all :
	@$(GPP) $(MAIN) $(SRC) -o programe
	@echo "Its done!"

clean:
	@rm -rf programe "*.out"
	@echo "Its done on [`date`]"
